		"prepare": "husky install"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.16",
		"@headlessui/tailwindcss": "^0.2.0",
		"@next/bundle-analyzer": "^13.4.12",
		"@types/node": "20.4.5",
		"@types/react": "18.2.18",
		"@types/react-dom": "18.2.7",
		"eslint": "8.46.0",
		"eslint-config-next": "^13.4.12",
		"keen-slider": "^6.8.6",
		"next": "^13.4.12",
		"next-themes": "^0.2.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"recoil": "^0.7.7",
		"sharp": "^0.32.4",
		"stripe": "^12.16.0",
		"typescript": "^5.1.6"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.14",
		"cspell": "^6.31.2",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.3",
		"postcss": "^8.4.27",
		"prettier": "^3.0.0",
		"prettier-plugin-tailwindcss": "0.4.1",
		"tailwindcss": "^3.3.3"
	},
	"lint-staged": {
		"*": [
			"prettier --write --ignore-unknown --cache --cache-location=.prettier-cache"
