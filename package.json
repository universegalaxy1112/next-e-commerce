{
	"name": "e-commerce-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"dev:turbo": "pnpm -s dev --turbo",
		"build": "next build",
		"build:analyze": "set ANALYZE=true&& pnpm build",
		"start": "next start",
		"lint": "next lint",
		"ts:errors": "tsc --noEmit",
